{
  "configurations": [
  {
    "name": "(gdb) Lancer",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/builddir/dune3d",
    "args": [],
    "stopAtEntry": false,
    "cwd": "${fileDirname}",
    "environment": [],
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
        {
            "description": "Activer l'impression en mode Pretty pour gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description": "Définir la version désassemblage sur Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
        }
    ]

  }
  ]
}